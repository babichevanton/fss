<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="springapp.fss.pojo.Seat" table="Seat">
		<id name="id" type="int" column="idSeat" >
			<generator class="identity"/>
		</id>
	
		<property name="row" column="row" type="short"/>
		<property name="symb" column="symb" type="string"/>
		<property name="status" column="status" type="byte"/>

		<many-to-one name="ServiceClass" lazy="false" class="springapp.fss.pojo.ServiceClass" column="class"/>
		<many-to-one name="Owner" lazy="false" class="springapp.fss.pojo.Client" column="owner"/>
	</class>
</hibernate-mapping>