<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="springapp.fss.pojo.ParticularFlight" table="ParticularFlight">
		<id name="id" type="int" column="idParticularFlight" >
			<generator class="identity"/>
		</id>
	
		<property name="dptr" column="dptr_time" type="calendar"/>
		<property name="arr" column="arr_time" type="calendar"/>

		<many-to-one name="Flight" class="springapp.fss.pojo.Flight" column="flight"/>

		<set name="Services" table="ServiceClass" inverse="true" lazy="false">
			<key column="particular_flight"/>
			<one-to-many class="springapp.fss.pojo.ServiceClass"/>
		</set>
	</class>
</hibernate-mapping>